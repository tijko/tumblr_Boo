{
    "manifest_version": 2,
    "name": "Tumblr Boo!",
    "description": "Blocks 'booed' posts from your tumblr dashboard",
    "version": "1.0",

    "content_scripts": [
    {
        "matches": ["http://www.tumblr.com/dashboard*"],
        "js":["boo.js"],
        "css":["boo.css"]
    }
    ],
    
    "permissions": ["storage"],
    "web_accessible_resources":[
        "images/boo.png"
    ]
}

